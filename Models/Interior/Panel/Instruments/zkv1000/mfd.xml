  <!-- Map management  
  <animation>
    <type>texmultiple</type>
    <object-name>map.background</object-name>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/map/moving-x</property>
      <factor>0.6667</factor>
      <axis>
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </transform>
    <transform>
      <subtype>textranslate</subtype>
      <property>/instrumentation/zkv1000/map/moving-y</property>
      <factor>0.6667</factor>
      <axis>
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </axis>
    </transform>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>map.background</object-name>
    <property-base>/instrumentation/zkv1000/map</property-base>
    <texture-prop>terrain-path</texture-prop>
  </animation>
  -->
  
  <animation>
    <type>rotate</type>
    <object-name>map.aircraft</object-name>
    <property>/orientation/heading-deg</property>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>map.alt</object-name>
    <property>/instrumentation/zkv1000/map/alt</property>
    <factor> 0.006873333 </factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>map.alt.selected</object-name>
    <property>/instrumentation/zkv1000/map/alt-selected</property>
    <factor> 0.006873333 </factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>map.alt.1min</object-name>
    <property>/instrumentation/zkv1000/map/alt-1-min</property>
    <factor> 0.006873333 </factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <!-- LOAD INDICATOR -->
  <animation>
    <type>scale</type>
    <object-name>infos.leftLoad</object-name>
    <property>/instrumentation/zkv1000/mfd/thrust0</property>
    <x-factor>  0.000 </x-factor>
    <y-factor>  0.000 </y-factor>
    <z-factor> 86.035 </z-factor>
    <x-offset>  1.000 </x-offset>
    <y-offset>  1.000 </y-offset>
    <z-offset>  1.000 </z-offset>
    <center>
      <x-m>  0.00689 </x-m>
      <y-m> -0.10262 </y-m>
      <z-m>  0.04689 </z-m>
    </center>
  </animation>
  
  <animation>
    <type>translate</type>
    <object-name>cursor.leftLoad</object-name>
    <property>/instrumentation/zkv1000/mfd/thrust0</property>
    <factor> 0.0257 </factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
  
