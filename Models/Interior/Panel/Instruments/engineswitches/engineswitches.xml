  <path>engineswitches.ac</path>
  
  <!--ELECTRIC BUS-->
  <!--Master Electric Switch-->
  <animation>
    <type>pick</type>
    <object-name>electricmasterswitch</object-name>
    <action>
      <button> 0 </button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/electric/master</property>
      </binding>
    </action>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>electricmasterswitch</object-name>
        <property>controls/electric/master</property>
        <factor>-50</factor>
        <center>
            <x-m>-5.09165</x-m>
            <z-m>-1.18698</z-m>
        </center>
        <axis>
            <y>1</y>
            </axis>
  </animation>
