  <!--ENGINE SWITCHES-->
  <!--Engine Left Master switch-->
  <animation>
    <type>pick</type>
    <object-name>engLswitch</object-name>
    <action>
      <button> 0 </button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine[0]/master-switch</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine[0]/cutoff</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>controls/engines/engine[0]/fuel-pump</property>
      </binding>
      <binding>
        <command>property-cycle</command>
        <property>controls/engines/engine[0]/condition</property>
        <value>1</value>
        <value>0</value>
      </binding>
      <binding>
        <command>property-cycle</command>
        <property>controls/engines/engine[0]/propeller-pitch</property>
        <value>1</value>
        <value>0</value>
      </binding>
    </action>
  </animation>
  <animation>
