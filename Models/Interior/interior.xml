<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <!-- Planche de bord -->
  <model>
    <path>Aircraft/EV55/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -5.09 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -1.23647 </z-m>
    </offsets>
  </model>
  <!--PANEL-->
  <!--Engine switches-->
  <model>
    <path>Aircraft/EV55/Models/Interior/Panel/Instruments/engineswitches/engineswitches.xml</path>
  </model>  
  <!--Gear Indicator-->
  <!--Illuminate "UP" Indicators-->
   <animation>
    <type>material</type>
    <object-name>gearFup</object-name>
    <condition><less-than>
            <property>/gear/gear/position-norm</property>
            <value>1</value>
        </less-than>
    </condition>
    <ambient>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearLup</object-name>
    <condition><less-than>
            <property>/gear/gear[1]/position-norm</property>
            <value>1</value>
        </less-than>
    </condition>
    <ambient>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearRup</object-name>
    <condition><less-than>
            <property>/gear/gear[2]/position-norm</property>
            <value>1</value>
        </less-than>
    </condition>
    <ambient>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </emission>
  </animation>
  <!--Illuminate "DOWN" Indicators-->
   <animation>
    <type>material</type>
    <object-name>gearFdown</object-name>
    <condition><equals>
            <property>/gear/gear/position-norm</property>
            <value>1</value>
        </equals>
    </condition>
    <ambient>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearLdown</object-name>
    <condition><equals>
            <property>/gear/gear[1]/position-norm</property>
            <value>1</value>
        </equals>
    </condition>
    <ambient>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearRdown</object-name>
    <condition><equals>
            <property>/gear/gear[2]/position-norm</property>
            <value>1</value>
        </equals>
    </condition>
    <ambient>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </emission>
  </animation>
<!--Mixture and Throttle-->
<!--Throttle-->
    <animation>
        <type>rotate</type>
        <object-name>throttleL</object-name>
        <property>/controls/engines/engine[0]/throttle</property>
        <factor>-70</factor>
        <center>
            <x-m>-4.957</x-m>
            <z-m>-1.3269</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>throttleR</object-name>
        <property>/controls/engines/engine[1]/throttle</property>
        <factor>-70</factor>
        <center>
            <x-m>-4.957</x-m>
            <z-m>-1.3269</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0.0</z>
        </axis>
    </animation>
  <!--Mixture-->

    <animation>
        <type>rotate</type>
        <object-name>mixtureL</object-name>
        <property>/controls/engines/engine[0]/mixture</property>
        <factor>-70</factor>
        <center>
            <x-m>-4.957</x-m>
            <z-m>-1.3269</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0.0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>mixtureR</object-name>
        <property>/controls/engines/engine[1]/mixture</property>
        <factor>-70</factor>
        <center>
            <x-m>-4.957</x-m>
            <z-m>-1.3269</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0.0</z>
        </axis>
    </animation>
</PropertyList>
