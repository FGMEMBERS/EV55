<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <!-- Planche de bord -->
  <model>
    <path>Aircraft/EV55/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -5.09 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m> -1.23647 </z-m>
    </offsets>
  </model>
   <animation>
    <type>material</type>
    <object-name>interior</object-name>
    <object-name>seats</object-name>
    <object-name>doorFLint</object-name>
    <object-name>doorFRint</object-name>
    <object-name>doorR1int</object-name>
    <object-name>doorR1Dint</object-name>
    <object-name>doorR2int</object-name>
    <condition>
      <property>systems/electrical/power</property>
            <property>/controls/lighting/cabin-dome</property>
    </condition>
    <emission>
        <red>0.5</red>
        <green>0.5</green>
        <blue>0.5</blue>
    </emission>
  </animation>
  <!--PANEL-->
   <animation>
    <type>material</type>
    <object-name>Panel.002</object-name>
    <object-name>Panel.003</object-name>
    <object-name>Middleconsole</object-name>
    <condition>
      <property>systems/electrical/power</property>
            <property>/controls/lighting/instrument-lights</property>
    </condition>
    <emission>
        <red>1</red>
        <green>1</green>
        <blue>1</blue>
        <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    </emission>
  </animation>
  
  <!--Engine switches-->
  <model>
    <path>Aircraft/EV55/Models/Interior/Panel/Instruments/engineswitches/engineswitches.xml</path>
  </model>  
  <!--Gear Indicator-->
  <!--Illuminate "UP" Indicators-->
   <animation>
    <type>material</type>
    <object-name>gearFup</object-name>
    <condition><less-than>
            <property>/gear/gear/position-norm</property>
            <value>1</value>
        </less-than>
    </condition>
    <ambient>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearLup</object-name>
    <condition><less-than>
            <property>/gear/gear[1]/position-norm</property>
            <value>1</value>
        </less-than>
    </condition>
    <ambient>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearRup</object-name>
    <condition><less-than>
            <property>/gear/gear[2]/position-norm</property>
            <value>1</value>
        </less-than>
    </condition>
    <ambient>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>1</red>
        <green>0</green>
        <blue>0</blue>
    </emission>
  </animation>
  <!--Illuminate "DOWN" Indicators-->
   <animation>
    <type>material</type>
    <object-name>gearFdown</object-name>
    <condition><equals>
            <property>/gear/gear/position-norm</property>
            <value>1</value>
        </equals>
    </condition>
    <ambient>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearLdown</object-name>
    <condition><equals>
            <property>/gear/gear[1]/position-norm</property>
            <value>1</value>
        </equals>
    </condition>
    <ambient>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </emission>
  </animation>
   <animation>
    <type>material</type>
    <object-name>gearRdown</object-name>
    <condition><equals>
            <property>/gear/gear[2]/position-norm</property>
            <value>1</value>
        </equals>
    </condition>
    <ambient>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </ambient>
    <emission>
        <red>0</red>
        <green>1</green>
        <blue>0</blue>
    </emission>
  </animation>
<!--Flapslever-->
 <animation>
    <type>slider</type>
        <object-name>flapslever</object-name>
    <drag-direction>vertical</drag-direction>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/flight/flaps</property>
        <factor>-0.05</factor>
        <min>0</min>
        <max>1</max>
        <wrap>0</wrap>
      </binding>
    </action>
    <hovered>
      <binding>
        <command>set-tooltip</command>
        <label>Flaps: %3d%%</label>
        <measure-text>Flaps: 100%</measure-text>
        <tooltip-id>flaps</tooltip-id>
        <mapping>percent</mapping>
        <property>controls/flight/flaps</property>
      </binding>
    </hovered>
  </animation>
    <animation>
        <type>rotate</type>
        <object-name>flapslever</object-name>
        <property>controls/flight/flaps</property>
        <factor>32</factor>
        <center>
            <x-m>-4.8053</x-m>
            <z-m>-1.4819</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0.0</z>
        </axis>
    </animation>
